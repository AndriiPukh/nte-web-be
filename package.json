{
  "name": "nte-web-be",
  "version": "1.0.0",
  "description": "Second try for NTE",
  "main": "server.js",
  "scripts": {
    "serve": "nodemon src/server.js",
    "test": "jest --passWithNoTests",
    "lint": "eslint --debug src/",
    "lint:write": "eslint --debug src/ --fix",
    "prettier": "prettier --write src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/AndriiPukh/nte-web-be.git"
  },
  "author": "Andrii Pukh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AndriiPukh/nte-web-be/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "npm run lint:write",
      "git add",
      "npm run test"
    ]
  },
  "homepage": "https://github.com/AndriiPukh/nte-web-be#readme",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jest": "^29.4.3",
    "nodemon": "^2.0.21"
  },
  "devDependencies": {
    "eslint": "^8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4"
  }
}
